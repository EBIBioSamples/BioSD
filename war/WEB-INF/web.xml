<web-app xmlns:javaee="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>ESD.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <javaee:description></javaee:description>
    <javaee:display-name>QueryServiceImpl</javaee:display-name>
    <servlet-name>QueryServiceImpl</servlet-name>
    <servlet-class>uk.ac.ebi.esd.server.service.ESDServiceImpl</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>AgeAdminServiceImpl</display-name>
  	<servlet-name>AgeAdminServiceImpl</servlet-name>
  	<servlet-class>
  	uk.ac.ebi.age.admin.server.service.AgeAdminServiceImpl</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>UploadSvc</display-name>
  	<servlet-name>UploadSvc</servlet-name>
  	<servlet-class>
  	uk.ac.ebi.age.admin.server.service.UploadSvc</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QueryServiceImpl</servlet-name>
    <url-pattern>/ageQueryGWT</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AgeAdminServiceImpl</servlet-name>
  	<url-pattern>/uk.ac.ebi.age.admin.Admin/ageAdminGWT</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UploadSvc</servlet-name>
  	<url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>uk.ac.ebi.esd.server.service.Init</listener-class>
  </listener>
</web-app>
